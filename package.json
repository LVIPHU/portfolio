  "scripts": {
    "dev": "next dev",
    "build": "cross-env INIT_CWD=$PWD next build && next build",
    "postbuild": "cross-env NODE_OPTIONS='--experimental-json-modules' tsx ./scripts/post-build.ts",
    "deploy": "npm run db:generate && next build && cross-env NODE_OPTIONS='--experimental-json-modules' tsx ./scripts/post-build.ts",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "db:generate": "npx drizzle-kit generate ",
    "db:push": "npx drizzle-kit push",
    "db:init": "npm run db:generate && npm run db:push",
    "db:studio": "npx drizzle-kit studio --port 8088",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:write": "prettier --write --ignore-path .gitignore .",
